{
  "name": "comp3663-vuejs",
  "version": "1.0.0",
  "description": "Frontend for this weather visualization project",
  "author": "Weather Team",
  "private": true,
  "scripts": {
    "dev": "nuxt",
    "build": "nuxt build",
    "start": "nuxt start",
    "generate": "nuxt generate",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "precommit": "npm run lint",
    "test:unit": "TEST=unit yarn ava ./test/specs/*.js --verbose",
    "test:e2e": "TEST=e2e yarn ava ./test/e2e/*.js --verbose",
    "test": "yarn test:unit & yarn test:e2e"
  },
  "dependencies": {
    "@nuxtjs/axios": "^5.3.6",
    "@nuxtjs/pwa": "^2.6.0",
    "chart.js": "^2.7.3",
    "cross-env": "^5.2.0",
    "nuxt": "^2.3.4",
    "vue-chartjs": "^3.4.0",
    "vuetify": "^1.5.4"
  },
  "devDependencies": {
    "@nuxtjs/eslint-config": "^0.0.1",
    "@vue/test-utils": "^1.0.0-beta.29",
    "ava": "^0.25.0",
    "babel-eslint": "^8.2.1",
    "babel-plugin-module-resolver": "^3.1.1",
    "babel-preset-es2015": "^6.24.1",
    "browser-env": "^3.2.6",
    "eslint": "^5.0.1",
    "eslint-config-standard": ">=12.0.0",
    "eslint-loader": "^2.0.0",
    "eslint-plugin-ava": "^5.1.1",
    "eslint-plugin-import": ">=2.14.0",
    "eslint-plugin-jest": ">=21.24.1",
    "eslint-plugin-node": ">=7.0.1",
    "eslint-plugin-promise": ">=4.0.1",
    "eslint-plugin-standard": ">=4.0.0",
    "eslint-plugin-vue": "^5.0.0",
    "jsdom": "^14.0.0",
    "nodemon": "^1.18.9",
    "require-extension-hooks": "^0.3.2",
    "require-extension-hooks-babel": "^0.1.1",
    "require-extension-hooks-vue": "^0.4.1",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.2"
  },
  "ava": {
    "require": [
      "babel-register",
      "./test/helpers/setup.js"
    ],
    "snapshotDir": "./test/snapshot"
  },
  "babel": {
    "presets": [
      "es2015"
    ],
    "plugins": [
      [
        "module-resolver",
        {
          "root": [
            "./src"
          ],
          "alias": {
            "@": "./",
            "~": "./"
          }
        }
      ]
    ]
  }
}
